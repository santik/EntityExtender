{
    "name": "santik/entity-extender",
    "description": "Library for extending entities with new fields",
    "license": "proprietary",
    "authors": [
        {
              "name": "Alexander Fedortets",
              "email": "fedorets.alex@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "doctrine/orm": "^2.5",
        "symfony/event-dispatcher": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Santik\\EntityExtender\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Santik\\EntityExtender\\": ["tests/unit/"]
        }
    },
    "scripts": {
        "fix-style": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit"
    }
}
